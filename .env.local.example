DB_HOST=localhost
DB_PORT=3306
DB_NAME=platform_log
DB_USER=
DB_PASSWORD=
NODE_ENV=development
PORT=3000

# 对账单每页默认显示数量
RECONCILIATION_PAGE_SIZE=20
NEXT_PUBLIC_RECONCILIATION_PAGE_SIZE=20

# 对账单自动生成时间（24小时制）
RECONCILIATION_AUTO_GENERATE_TIME=01:00
# 对账单存储目录
RECONCILIATION_EXPORT_DIR=./data/reconciliation

# 对账单数据来源方式: 'generate'=即时生成, 'file'=使用预生成文件
RECONCILIATION_SOURCE_TYPE=generate
NEXT_PUBLIC_RECONCILIATION_SOURCE_TYPE=generate

# 数据库表名配置
SERVICE_LOGS_TABLE=t_service_log
ORGANIZATIONS_TABLE=t_org_info

# 表列名映射配置，根据实际数据表结构进行修改
COLUMN_ORG_NAME=org_name
COLUMN_ORG_ID=org_id
COLUMN_AUTH_MODE=auth_mode
COLUMN_EXEC_START_TIME=exec_start_time
COLUMN_RESULT_CODE=result_code
COLUMN_RESULT_MSG=result_msg
NEXT_PUBLIC_API_MOCK=false

# API基础URL，用于构建下载链接
NEXT_PUBLIC_API_URL=http://localhost:3000